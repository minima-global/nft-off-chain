    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && zip -r minima-nft-offchain.minidapp build",
        "build-zip": "npm run build && rm nft_* && date=$(date +%Y_%m_%d_%H%M) && cd build && zip -r nft_${date}.zip . && mv nft_${date}.zip ../",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "./",
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "printWidth": 120
    },
    "devDependencies": {
        "@types/lodash": "^4.14.175"
    }
